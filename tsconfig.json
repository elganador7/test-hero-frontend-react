{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node", "vite/client"]
    },
    "include": ["src"]
  }
  